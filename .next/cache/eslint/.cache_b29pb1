\n                        <div className={styles.new_img_box}>\r\n                            <Image className={styles.new_img} src={item.imgSrc} width={400} height={item.imgH}></Image>\r\n                        </div>\r\n                        <h3 className={styles.new_title}>{item.title}</h3>\r\n                        <div className={styles.new_footer}>\r\n                            <p className={styles.new_footer_date}>Publicado: {item.date}</p>\r\n                            <span>•</span>\r\n                            <button aria-label={`Leer ${item.title}`} className={styles.new_footer_btn}>Leer más →</button>\r\n                        </div>\r\n                    </article>\r\n                )}\r\n            />\r\n        </div>\r\n    </section>\r\n  )\r\n}\r\n","C:\\Users\\Usuario\\repositories\\Challenge-Demo\\src\\components\\generals\\Footer.jsx",["144","145"],[],"import styles from \"@/styles/components/Footer.module.scss\"\r\nimport Link from \"next/link\"\r\nimport { Bodoni_Moda } from 'next/font/google'\r\nimport { pages, social } from \"@/pages/api/data\"\r\nimport Image from \"next/image\"\r\n\r\nconst bodoni = Bodoni_Moda({ subsets: ['latin'] })\r\n\r\nexport default function Footer() {\r\n  return (\r\n    <footer className={styles.footer}>\r\n        <div className={styles.footer_menu}>\r\n            <ul className={styles.footer_menu_list}>\r\n                {\r\n                    pages.map((item)=>{\r\n                        let idx = \"foot_\"+item.id;\r\n\r\n                        return(\r\n                            <li key={idx} className={styles.footer_menu_item}>\r\n                                <Link href=\"/\">{item.title}</Link>\r\n                            </li>\r\n                        )\r\n                    })\r\n                }\r\n            </ul>\r\n        </div>\r\n\r\n        <div className={styles.footer_content}>\r\n            <div className={styles.footer_social}>\r\n                {\r\n       